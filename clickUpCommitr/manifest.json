{
    "version": "1.0.0",
    "name": "ClickUp Commitr",
    "permissions": [
        "activeTab"
    ],
    "browser_action": {
        "default_title": "ClickUp Commitr"
    },
    "content_scripts": [{
        "matches": ["https://app.clickup.com/*"],
        "js": ["jquery-3.2.1.min.js", "content.js"],
        "run_at": "document_end"
    }],
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "manifest_version": 2
}